{
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "no-mixed-spaces-and-tabs": "error",
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "parser": "babel-eslint",
    "ecmaFeatures": {
        "jsx": true,
    },
    "plugins": [
        "react"
    ]
}
